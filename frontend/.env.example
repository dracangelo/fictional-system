# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_TIMEOUT=10000

# Authentication
VITE_JWT_SECRET_KEY=your-jwt-secret-key-here
VITE_JWT_REFRESH_INTERVAL=900000

# External Services
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID

# WebSocket Configuration
VITE_WS_BASE_URL=ws://localhost:8000/ws

# File Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,video/mp4

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_GEOLOCATION=true
VITE_ENABLE_PWA=false

# Development Configuration
VITE_DEBUG_MODE=false
VITE_MOCK_API=false

# Social Login (Optional)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here

# Notification Services
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_sender_id_here