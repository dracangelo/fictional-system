{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "test:unit": "vitest --run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest --run src/**/*.integration.test.{ts,tsx}",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headed": "cypress run --headed",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:accessibility": "vitest --run src/test/accessibility*.test.tsx",
    "test:performance": "vitest --run src/test/performance*.test.ts",
    "test:components": "vitest --run src/components/**/*.test.{ts,tsx}",
    "test:hooks": "vitest --run src/hooks/**/*.test.{ts,tsx}",
    "test:services": "vitest --run src/services/**/*.test.{ts,tsx}",
    "test:pages": "vitest --run src/pages/**/*.test.{ts,tsx}",
    "test:all": "node scripts/test-runner.js",
    "test:suite": "node scripts/test-runner.js --suite",
    "test:ci": "npm run test:coverage && npm run test:accessibility && npm run test:performance && npm run test:e2e && npm run test:visual",
    "test:quick": "npm run test:unit && npm run test:accessibility",
    "test:debug": "vitest --run --reporter=verbose",
    "clean": "rm -rf dist node_modules/.cache coverage test-results cypress/screenshots cypress/videos playwright-report",
    "clean:cache": "rm -rf node_modules/.cache .vite",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "analyze:size": "npm run build -- --mode analyze",
    "build:stats": "npm run build -- --mode production --bundleAnalyzer",
    "build:optimize": "node scripts/build-optimize.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npx audit-ci --moderate"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.0.0",
    "@tanstack/query-sync-storage-persister": "^5.90.5",
    "@tanstack/react-query": "^5.90.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/testing-library__jest-dom": "^6.0.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsdom": "^27.0.0",
    "lucide-react": "^0.545.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "socket.io-client": "^4.8.1",
    "vitest": "^3.2.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.15.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jest-axe": "^10.0.0",
    "msw": "^2.6.8",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "@vitest/coverage-v8": "^3.2.4",
    "playwright": "^1.49.1",
    "start-server-and-test": "^2.0.8"
  }
}
